a=int(input("ENTER THE NUMBER: "))
for i in range(a):
    a=a%2==0
    print(a)


a = int(input("ENTER THE NUMBER: "))
count = 0
for i in range(2, a):
    if a % i == 0:
        count = count + 1
        break
if count > 0:
    print("Composite")
else:
    print("Prime")
